# Mapfile describing the Sidecar platform implementation as symbols exported by
# the Dendrite repository itself.

$mapfile_version 2

SYMBOL_SCOPE {
	global:
		# QSFP module management functions.
		bf_pltfm_qsfp_init              { FLAGS = EXTERN };
		bf_pltfm_detect_qsfp            { FLAGS = EXTERN };
		bf_pltfm_qsfp_read_module       { FLAGS = EXTERN };
		bf_pltfm_qsfp_write_module      { FLAGS = EXTERN };
		bf_pltfm_qsfp_read_reg          { FLAGS = EXTERN };
		bf_pltfm_qsfp_write_reg         { FLAGS = EXTERN };
		bf_pltfm_qsfp_get_presence_mask { FLAGS = EXTERN };
		bf_pltfm_qsfp_get_int_mask      { FLAGS = EXTERN };
		bf_pltfm_qsfp_get_lpmode_mask   { FLAGS = EXTERN };
		bf_pltfm_qsfp_set_lpmode        { FLAGS = EXTERN };
		bf_pltfm_qsfp_module_reset      { FLAGS = EXTERN };
};
